<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
     <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
        <% if(user){ %>
        <form class="form-inline" action="/logout" method="GET">
          <label for="login"></label>
            <button type="submit" style="border-color:black; background-color:transparent; border-radius: 8px; padding: 5px 18px; ">Logout</button>
          </form>
          <% } else { %> 
            <form class="form-inline" action="/register" method="GET">
              <label for="register"></label>
                <button type="submit" style="border-color:black; background-color:transparent; border-radius: 8px; padding: 5px 18px; ">Register</button>
              </form>
      
            <form class="form-inline" action="/login" method="GET">
              <label for="login"></label>
                <button type="submit" style="border-color:black; background-color:transparent; border-radius: 8px; padding: 5px 18px; ">Login</button>
              </form>  
            
            
            <% } %> 
    </div>
  </nav>
</header>